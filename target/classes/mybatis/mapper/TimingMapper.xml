<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.auc.mapper.TimingMapper">
    <select id="queryProduce" resultType="com.auc.pojo.Vip">
        select vip_id,vip_account,vip_endtime from t_vip
    </select>

    <delete id="RemoveVip">
        delete from t_vip
        <where>
            vip_account=#{vipAccount}
        </where>
    </delete>

</mapper>